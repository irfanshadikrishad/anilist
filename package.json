{
  "name": "@irfanshadikrishad/anilist",
  "description": "Minimalist unofficial AniList CLI for Anime and Manga Enthusiasts",
  "author": "Irfan Shadik Rishad",
  "version": "1.3.0-forbidden.1",
  "main": "./bin/index.js",
  "type": "module",
  "types": "./bin/index.d.ts",
  "bin": {
    "anilist": "./bin/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rm -rf ./bin && tsc",
    "buildw": "rm -rf ./bin && tsc -w",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "eslint ./dist",
    "lint:fix": "eslint ./dist --fix",
    "all": "npm run lint && npm run lint:fix && npm run format && npm test",
    "test": "jest ./tests"
  },
  "keywords": [
    "anilist",
    "CLI",
    "anime",
    "manga",
    "anime list",
    "manga list",
    "anime tracker",
    "manga tracker",
    "anilist API",
    "anime progress",
    "manga progress",
    "media list",
    "export anime",
    "import anime",
    "export manga",
    "import manga",
    "status tracker",
    "watchlist",
    "reading list",
    "graphql"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/irfanshadikrishad/anilist"
  },
  "homepage": "https://github.com/irfanshadikrishad/anilist",
  "bugs": {
    "url": "https://github.com/irfanshadikrishad/anilist/issues"
  },
  "license": "MPL-2.0",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/jest": "^29.5.14",
    "@types/json2csv": "^5.0.7",
    "@types/node": "^22.10.5",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "@babel/preset-env": "^7.26.0",
    "@typescript-eslint/eslint-plugin": "^8.19.1"
  },
  "dependencies": {
    "commander": "^13.0.0",
    "fast-xml-parser": "^4.5.1",
    "inquirer": "^12.3.0",
    "json2csv": "^6.0.0-alpha.2",
    "node-fetch": "^3.3.2",
    "open": "^10.1.0"
  }
}
